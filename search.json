---
layout: null
---
{
  "docs": [
    {% assign pages = site.pages | where_exp:'p','p.search != false' %}
    {% for p in pages %}
    {
      "title": {{ p.title | jsonify }},
      "url": {{ p.url | jsonify }},
      "content": {{ p.content | strip_html | normalize_whitespace | jsonify }},
      "excerpt": {{ p.excerpt | strip_html | normalize_whitespace | jsonify }},
      "lang": {{ p.lang | default: site.lang | jsonify }}
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
